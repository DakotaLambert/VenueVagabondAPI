        "Rfkhsox",
        "authtoken",
        "branchname",
        "cigdxa",
        "imgstr",
        "userevent",
        "vvuser"
    ],
    "cSpell.words": [
        "ADTR",
        "eventimage",
        "eventtype",
        "TDWP"
    ],
}